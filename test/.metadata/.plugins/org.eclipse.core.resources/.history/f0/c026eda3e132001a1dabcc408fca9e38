    <script>
	function jumpToSignup(){
		var redirectUrl = '';
		var reg = new RegExp("(^|&)redirectUrl=([^&]*)(&|$)");
		var r = window.location.search.substr(1).match(reg);
		if (r) {
			redirectUrl = unescape(r[2]);
		}
		if(!redirectUrl){
			redirectUrl = window.location.href
		}
		var targetSignupUrl = '/guest/signup'
		if(redirectUrl.indexOf('signup') == -1 && redirectUrl.indexOf('signin') == -1){
			targetSignupUrl += "?redirectUrl=" +redirectUrl;
		}
		window.location.href = targetSignupUrl;
	}
</script>
<script>
    var NavbarFloat = (function () {
        function NavbarFloat() {
            var self = this;
            this.itemList = {};
            $(window).load(function() {
                self._initAppMenu();
                setTimeout(function() {
                    self.navbarImpl.addClass("display");
                }, 100);
            });
        }
        NavbarFloat.prototype._initAppMenu = function () {
            this._registerItem(//pxcook
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_pxcook.png",
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_pxcook@2x.png 2x",
                "PxCook","设计研发协作平台","/pxcook","#1ea6fc","pxcook",true);
            this._registerItem(//bigshear
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_bigshear.png",
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_bigshear@2x.png 2x",
                "BigShear","游戏图片分割，合并利器","/bigshear","#49d0ea","bigshear",true);
            this._registerItem(//colorcube
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_colorcube.png",
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_colorcube@2x.png 2x",
                "ColorCube","配色神器","/colorcube","#ff8948","colorcube",true);
            this._registerItem(//sweet_onion
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_sweet_onion.svg",null,
                "甜葱填图","高效批量图片填充-Ps插件","/sweet_onion","#7737e7","sweet_onion",false);
            this._registerItem(//flavor
                "https://web-assets.fancynode.com.cn/web_v1/images/fancynode/header/icon_flavor.svg",null,
                "Flavor","给你的设计加点料-Sketch插件","/flavor","#1ea6fc","flavor",false);

        }
        NavbarFloat.prototype._registerItem = function (icon,iconRetina,title,des,link,color,type,isApp) {
            var container = null;
            if(isApp){
                container = $(".navbar-float .app-list-container.app");
            }else{
                container = $(".navbar-float .app-list-container.plugin");
            }
            var item =
            "<li>"+
            "   <a href=\""+link+"\">";
            if(iconRetina){
                item +=
                "   <img class=\"icon\" src=\""+icon+"\" srcset=\""+iconRetina+"\"/>";
            }else{
                item +=
                "   <img class=\"icon\" src=\""+icon+"\" />";
            }
            item +=
            "       <div class=\"menu-text-container\">"+
            "           <span class=\"title\">"+title+"</span>"+
            "           <span class=\"des\">"+des+"</span>"+
            "       </div>"+
            "   </a>"+
            "</li>";
            var itemImpl = $(item).appendTo(container);

            this.itemList[type] = {
                "item":itemImpl,
                "color":color
            }
        }

        NavbarFloat.prototype._scroll_handler = function () {
            if(this.stillInContainer){
                this.navbarImpl.addClass("float");
                this.icon.attr("src",this.titleIconColor);
                this.title.css("color",this.titleColor);
                return;
            }
            var headerHeight = this.headerContainer.outerHeight();

            var type = "";
            if ($(window).scrollTop() <= headerHeight) {
                type = "header";
			} else {
                type = "container";
			}
            if(type != this.typeCache){
                this.typeCache = type;
                if(type == "header"){
                    this.navbarImpl.removeClass("float");
                    this.icon.attr("src",this.titleIcon);
                    this.title.css("color","inherit");
                }else{
                    this.navbarImpl.addClass("float");
                    this.icon.attr("src",this.titleIconColor);
                    this.title.css("color",this.titleColor);
                }
            }
        }
        NavbarFloat.prototype._resize_handler = function () {
            var containerWidth = this.navbarContainer.width();
            if(this.titleContainer.outerWidth(true)+this.itemContainer.outerWidth(true)+this.loginContainer.outerWidth(true)+30 >= containerWidth){
                this.itemContainer.fadeOut(100);
            }else{
                this.itemContainer.fadeIn(100);
            }
        }
        NavbarFloat.prototype.init = function (titleIcon,titleIconColor,title,titleColor,buttonClass,link,stillInContainer) {
            var self = this;
            $(window).load(function() {
                self.navbarImpl = $(".navbar-float");

                self.navbarContainer = $(".navbar-float .container");
                self.titleContainer = $(".navbar-float .zc-navbar-head");
                self.itemContainer = $(".navbar-float .navbar-item-list");
                self.loginContainer = $(".navbar-float .right-part");


                self.icon = $(".navbar-float .container .title-container .title-icon");
                self.title = $(".navbar-float .container .title-container .title");

                self.titleIcon = titleIcon;
                self.titleIconColor = titleIconColor;
                self.titleColor = titleColor;

                self.title.text(title);
                self.icon.attr("src",titleIcon);

                var type = "flavor"
                var activeItem = self.itemList[type];
                var itemColor = activeItem["color"];
                activeItem["item"].addClass("active");
                activeItem["item"].css("background-image","linear-gradient("+itemColor+","+itemColor+")");

                $(".navbar-float .container .title-container").attr("href",link);

				self.stillInContainer = stillInContainer;
				$(".navbar-float .signup").addClass(buttonClass)
            });
        }

        NavbarFloat.prototype.addNavbarItem = function (domStr,callBack) {
            $(window).load(function() {
                var item = $("<a class=\"navbar-item\">"+domStr+"</a>").appendTo($(".navbar-float .navbar-item-list"));
                if(callBack){
                    item.on("click",callBack);
                }
            });
        }
        NavbarFloat.prototype.setHeader = function (header) {
            var self = this;
            $(window).load(function() {
                self.headerContainer = $(header);
                self._scroll_handler();
                self._resize_handler();
                $(window).scroll(function () {
                    self._scroll_handler();
                });
                $(window).resize(function() {
                    self._resize_handler();
                });
            });
        }
        return NavbarFloat;
    }());
    var navbar = new NavbarFloat();
</script>
